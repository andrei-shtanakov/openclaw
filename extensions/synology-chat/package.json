{
  "name": "@orchid/synology-chat",
  "version": "2026.2.22",
  "private": true,
  "description": "Synology Chat channel plugin for Orchid",
  "type": "module",
  "devDependencies": {
    "orchid": "workspace:*"
  },
  "orchid": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "synology-chat",
      "label": "Synology Chat",
      "selectionLabel": "Synology Chat (Webhook)",
      "docsPath": "/channels/synology-chat",
      "docsLabel": "synology-chat",
      "blurb": "Connect your Synology NAS Chat to Orchid with full agent capabilities.",
      "order": 90
    },
    "install": {
      "npmSpec": "@orchid/synology-chat",
      "localPath": "extensions/synology-chat",
      "defaultChoice": "npm"
    }
  }
}
